@if (IsActive) {
    <MudChip T="string" Size="Size.Small" Color="Color.Success" IconColor="Color.Default">Active</MudChip>
}
else {
    <MudChip T="string" Size="Size.Small" Color="Color.Error" IconColor="Color.Default">Inactive</MudChip>
}
@code {
    [Parameter]
    public bool IsActive { get; set; }
}
